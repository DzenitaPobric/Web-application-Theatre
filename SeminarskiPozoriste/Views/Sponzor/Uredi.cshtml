@model Sponzor
@{
    ViewData["Title"] = "Uredi";

}
<h2 class="naslov">Uredi sponzora</h2>
<br />
<div class="okvir">
    <form asp-action="UrediSnimi" method="post">

        <input type="hidden" asp-for="SponzorID" />

        <div class="form-group">
            <label class="label">Naziv sponzora</label>
            <input class="form-control" type="text" asp-for="Naziv" id="input" />
        </div><br />

        <div class="form-group">
            <label class="label">Broj telefona</label>
            <input class="form-control" type="text" asp-for="BrojTelefona" id="input" />
        </div>

        <input type="submit" value="Sačuvaj" class="btn btn-danger" id="snimi" />
    </form>
    <br /><br />
    <a asp-action="Prikazi" class="btn btn-warning"><i class="fas fa-arrow-circle-left"></i>  Nazad</a>

</div>

<head>
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.8.2/css/all.css" integrity="sha384-oS3vJWv+0UjzBfQzYUhtDYW+Pj2yciDJxpsK1OYPAYjqT085Qq/1cq5FLXAZQ7Ay" crossorigin="anonymous">
    <link rel="stylesheet" href="~/css/DodajForm.css">

</head>
    

